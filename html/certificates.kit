	<body>
		<!-- @import nav.kit -->
		<header class="header wrapper">
			<p class="header__prefix">Certyfikaty</p>
			<h1 class="header__heading">Certyfikaty uzyskane przez PIBNowum.</h1>
			<img
				src="https://picsum.photos/640/400?random=1"
				class="header__image"
				alt=""
			/>
		</header>
